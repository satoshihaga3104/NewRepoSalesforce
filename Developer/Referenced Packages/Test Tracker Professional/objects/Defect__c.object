    <label>Defect</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>CREATEDBY_USER</columns>
        <columns>Assigned_To__c</columns>
        <columns>Close_Date__c</columns>
        <columns>Priority__c</columns>
        <columns>Severity__c</columns>
        <columns>Status_In_Image__c</columns>
        <columns>Duration__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
        <language>en_US</language>
    </listViews>
    <nameField>
        <displayFormat>D-{00000}</displayFormat>
        <label>Defect ID</label>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Defects</pluralLabel>
    <searchLayouts>
        <customTabListAdditionalFields>Summary__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>Severity__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>Assigned_To__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>Status__c</customTabListAdditionalFields>
        <lookupDialogsAdditionalFields>Severity__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>Target_Cycle__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>Status__c</lookupDialogsAdditionalFields>
        <lookupFilterFields>NAME</lookupFilterFields>
        <lookupFilterFields>Target_Cycle__c</lookupFilterFields>
        <searchFilterFields>Target_Cycle__c</searchFilterFields>
        <searchFilterFields>Severity__c</searchFilterFields>
        <searchResultsAdditionalFields>Assigned_To__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Detected_Cycle__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Status__c</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>Private</sharingModel>
    <sharingReasons>
        <fullName>TTProjectGroup__c</fullName>
        <label>TTProjectGroup</label>
    </sharingReasons>
    <validationRules>
        <fullName>Assignment_Should_Not_Blank</fullName>
        <active>true</active>
        <description>Defect should be assigned to any Tester</description>
        <errorConditionFormula>ISBLANK( Assigned_To__c )</errorConditionFormula>
        <errorMessage>Defect must be assigned to User!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cause_of_defect_mandatory</fullName>
        <active>true</active>
        <description>Before closing the defect, the cause of defect is mandatory</description>
        <errorConditionFormula>AND((TEXT( Status__c ) = &apos;Closed&apos; ),(TEXT( Cause_of_Defect__c)=Null))</errorConditionFormula>
        <errorMessage>Please enter the Cause of Defect, before closing the defect</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Defect_Status_Change</fullName>
        <active>true</active>
        <description>If the enhancement created, the defect status should not be changed.</description>
        <errorConditionFormula>IF(Enhancement_Created__c &amp;&amp; ISCHANGED( Status__c ),True,false)</errorConditionFormula>
        <errorMessage>Enhancement already created, can not change the status!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Detected_Cycle_Project_Code</fullName>
        <active>true</active>
        <description>Detected cycle must be in the same project as target cycle</description>
        <errorConditionFormula>IF(ISBLANK(Detected_Cycle__c),false,Detected_Cycle__r.Project_Code__c !=  Project_Code__c)</errorConditionFormula>
        <errorDisplayField>Detected_Cycle__c</errorDisplayField>
        <errorMessage>Please Choose the correct Project&apos;s Cycle!!.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Target_Cycle_Project_Code</fullName>
        <active>true</active>
        <description>Target Cycle must be in the same project.</description>
        <errorConditionFormula>IF(NOT(ISBLANK( Target_Cycle__c )),NOT( Project_Code__c == Target_Cycle__r.Project_Code__c ),false)</errorConditionFormula>
        <errorDisplayField>Target_Cycle__c</errorDisplayField>
        <errorMessage>Choose the correct project&apos;s Target Cycle!!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Test_Cycle_Required_for_any_Defects</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Test_Cycle__c )</errorConditionFormula>
        <errorMessage>Please choose the Test Cycle!!.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Create_Enhancement</fullName>
        <availability>online</availability>
        <description>If the defect needs enhancement then the enhancement needs to be created from the defect page itself</description>
        <displayType>button</displayType>
        <encodingKey>UTF-8</encodingKey>
        <linkType>url</linkType>
        <masterLabel>Create Enhancement</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <url>/apex/ttp__enhancementEdit?defectId={!Defect__c.Id}</url>
    </webLinks>
    <webLinks>
        <fullName>Link_Entity</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <encodingKey>UTF-8</encodingKey>
        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>Link Entity</masterLabel>
        <openType>sidebar</openType>
        <protected>false</protected>
        <url>/apex/ttp__addDefectLink?entityId={!Defect__c.Id}&amp;entity=defect</url>
    </webLinks>
</CustomObject>
